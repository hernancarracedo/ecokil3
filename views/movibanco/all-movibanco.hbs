        <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2><i class="fa fa-bank"></i> <strong> Detalle Cuenta Bancaria</strong></h2>
 
                </div>
                <div class="col-lg-2">

                </div>
            </div>
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                <div class="ibox ">
                    <div class="ibox-title">
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="fa fa-wrench"></i>
                            </a>
                            <ul class="dropdown-menu dropdown-user">
                                <li><a href="/movibanco/add" class="dropdown-item">Nuevo Movimiento</a>
                                </li>
                             
                            </ul>
                            <a class="close-link">
                                <i class="fa fa-times"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">

                        <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover dataTables-example" >
                    <thead>
                    <tr>
                                            <th>fecha</th>
                                            <th>monto</th>
                                            <th>comprob</th>
                                            <th>id concept</th>
                                            <th>tx concept</th>
                                            <th>concepto</th>
                                            <th>referencia</th>
                                            <th>saldo</th>
                                            <th>observaciones</th>
                                            <th>ACCIONES</th>
                    </tr>
                    </thead>
                    <tbody>
                                        {{#each resultado}}
                                        <tr>
                                            <td>{{ this.fecha }} </td>  
                                            <td>{{ this.monto }} </td>
                                            <td>{{ this.nro_comprob }} </td>
                                            <td>{{ this.id_movibanco_concepto }} </td>
                                            <td>{{ this.tx_movibanco_concepto }} </td>
                                            <td>{{ this.concepto }} </td>
                                            <td>{{ this.referencia }} </td>
                                            <td>{{ this.saldo }} </td>
                                            <td>{{ this.observaciones }} </td>
                                            
                                            <td class="center">
                                                <div class="btn-group">
                                                    <a href="/movibanco/edit/{{this.id_movibanco}}" class="btn-white btn btn-xs"><i class="fa fa-pencil"></i>&nbsp;Editar</a>
                                                    <form action="/movibanco/delete/{{this.id_movibanco}}?_method=PUT" method="POST">
                                                        <input type="hidden" name="_method" value="DELETE">
                                                        <button class="btn-white btn btn-xs"><i class="fa fa-trash"></i>&nbsp;Eliminar</button>
                                                    </form>
                                                </div>
                                            </td>
                                        </tr>
                                        {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                                            <th>fecha</th>
                                            <th>monto</th>
                                            <th>comprob</th>
                                            <th>id concept</th>
                                            <th>tx concept</th>
                                            <th>concepto</th>
                                            <th>referencia</th>
                                            <th>saldo</th>
                                            <th>observaciones</th>
                                            <th>ACCIONES</th>
                    </tr>
                    </tfoot>
                    </table>
                        </div>

                    </div>
                </div>
            </div>
            </div>
        </div>