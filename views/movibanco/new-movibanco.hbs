        <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2><i class="fa fa-bank"></i> <strong>Registro Nuevo Movimiento Bancario</strong></h2>
               
                </div>
                <div class="col-lg-2">

                </div>
            </div>
        <div class="wrapper wrapper-content animated fadeInRight">
 
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-content">

                            <form action="/movibanco/new-movibanco" method="POST">
                                <div class="col-sm-12"></div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Fecha</label>
                                    <div class="col-sm-10"><input type="date" name="fecha" class="form-control" placeholder="Ingrese fecha" /></div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Monto</label>
                                    <div class="col-sm-10"><input type="text" name="monto" class="form-control" placeholder="Ingese Monto" /></div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Nro Comprob.</label>
                                    <div class="col-sm-10"><input type="text" name="nro_comprob" class="form-control" placeholder="Nro de Comrobante" /></div>
                                </div>                             

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Concepto</label>
                                    <div class="col-md-10">    
                                        <select  name ="id_movibanco_concepto" class="form-control show-tick">
                                            <option value="">-- Sin Datos del Concepto --</option>
                                            {{#each result_concepto}}
                                                <option value="{{this.id_movibanco_concepto}}">{{this.tx_movibanco_concepto}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>

                         
                                     
                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Concepto</label>
                                    <div class="col-sm-10"><input type="text" name="concepto" class="form-control" placeholder="Concepto" /></div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Referencia</label>
                                    <div class="col-sm-10"><input type="text" name="referencia" class="form-control" placeholder="Referencia" /></div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Saldo</label>
                                    <div class="col-sm-10"><input type="text" name="saldo" class="form-control" placeholder="Saldo" /></div>
                                </div>                                                               

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Observaciones</label>
                                    <div class="col-sm-10"><input type="text" name="observaciones" class="form-control" placeholder="Obsevaciones" /></div>
                                </div>
            
                                <div class="hr-line-dashed"></div>                                                                                              
                                    <button class="btn btn-primary btn-block" type="submit">Guardar Nuevo Movimiento Bancario</button>
                                </div>
                            </form>



                        </div>
                    </div>
                </div>
            </div>
        </div>