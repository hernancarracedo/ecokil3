 
       <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2><i class="fa fa-sign-out"></i> Gastos</h2>
 
                </div>
                <div class="col-lg-2">

                </div>
            </div>
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                <div class="ibox ">
                    <div class="ibox-title">
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="fa fa-wrench"></i>
                            </a>
                            <ul class="dropdown-menu dropdown-user">
                                <li><a href="/gasto/add" class="dropdown-item">Nuevo</a>
                                </li>
                             
                            </ul>
                            <a class="close-link">
                                <i class="fa fa-times"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">

                        <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover dataTables-example" >
                    <thead>
                    <tr>
                                            <th>id_gasto</th>
                                            <th>id_tipo</th>
                                            <th>fecha</th>
                                            <th>observaciones</th>
                                            <th>monto</th>
                                            <th>id_billetera</th>
                                            <th>id_forma_pago</th>
                                            <th>vencimiento</th>
                                            <th>baja</th>
                                            <th>ACCIONES</th>
                    </tr>
                    </thead>
                    <tbody>
                                        {{#each resultado}}
                                        <tr>
                                            <td>{{ this.id_gasto}} </td>
                                            <td>{{ this.id_tipo }} </td>
                                            <td>{{ this.fecha }} </td>
                                            <td>{{ this.observaciones }} </td>
                                            <td>{{ this.monto }} </td>
                                            <td>{{ this.id_billetera }} </td>
                                            <td>{{ this.id_forma_pago }} </td>
                                            <td>{{ this.vencimiento }} </td>
                                            <td>{{ this.baja }} </td>  
                                            <td class="center">
                                                <div class="btn-group">
                                                    <a href="/gasto/edit/{{this.id_gasto}}" class="btn-white btn btn-xs">Editar</a>
                                                    <form action="/gasto/delete/{{this.id_gasto}}?_method=DELETE" method="POST">
                                                        <input type="hidden" name="_method" value="DELETE">
                                                        <button class="btn-white btn btn-xs">Eliminar</button>
                                                    </form>
                                                </div>
                                            </td>
                                        </tr>
                                        {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                                            <th>id_gasto</th>
                                            <th>id_tipo</th>
                                            <th>fecha</th>
                                            <th>observaciones</th>
                                            <th>monto</th>
                                            <th>id_billetera</th>
                                            <th>id_forma_pago</th>
                                            <th>vencimiento</th>
                                            <th>baja</th>
                                            <th>ACCIONES</th>
                    </tr>
                    </tfoot>
                    </table>
                        </div>

                    </div>
                </div>
            </div>
            </div>
        </div>