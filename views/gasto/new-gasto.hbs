        <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2><i class="fa fa-sign-out"></i> Ingreso de Nuevo Gasto</h2>
               
                </div>
                <div class="col-lg-2">

                </div>
            </div>
        <div class="wrapper wrapper-content animated fadeInRight">
 
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox ">
                        <div class="ibox-content">

                            <form action="/gasto/new-gasto" method="POST">
                                <div class="col-sm-12"></div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Categoria Gasto</label>
                                    <div class="col-md-10">    
                                        <select  name ="id_tipo" class="form-control show-tick">
                                            <option value="">-- Seleccione tipo gasto --</option>
                                            {{#each result_categorias}}
                                                <option value="{{this.id_tipo}}">{{this.categoria}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>
                                     
                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Fecha</label>
                                    <div class="col-sm-10"><input type="date" class="form-control" name="fecha" placeholder="Fecha" ></div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Observaciones</label>
                                    <div class="col-sm-10"><input type="text"  name="observaciones" class="form-control" placeholder="Obsevaciones" /></div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Monto</label>
                                    <div class="col-sm-10"><input type="text"  name="monto" class="form-control" placeholder="Monto" /></div>
                                </div>

                                 <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Billetera</label>
                                    <div class="col-md-10">    
                                        <select  name ="id_billetera" class="form-control show-tick">
                                            <option value="">-- Seleccione quien pago --</option>
                                            {{#each result_billetera}}
                                                <option   value="{{this.id_billetera}}">{{this.tx_billetera}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Forma de Pago</label>
                                    <div class="col-md-10">    
                                        <select  name ="id_forma_pago" class="form-control show-tick">
                                            <option value="">-- Seleccione forma de pago --</option>
                                            {{#each result_formapago}}
                                                <option   value="{{this.id_forma_pago}}">{{this.tx_forma_pago}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>                                

                                <div class="form-group row">
                                    <label class="col-sm-2 col-form-label">Vencimiento (opcional)</label>
                                    <div class="col-sm-10"> <input type="text"  name="vencimiento" class="form-control" placeholder="Vencimiento en caso de Servicios / Impuestos" /></div>
                                </div>
        
            
                                <div class="hr-line-dashed"></div>                                                                                              
                                    <button class="btn btn-primary btn-block" type="submit">Guardar Nuevo Gasto</button>
                                </div>
                            </form>



                        </div>
                    </div>
                </div>
            </div>
        </div>
 